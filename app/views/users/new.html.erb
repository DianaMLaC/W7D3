<h1>New User</h1>

<a class="button" href="<%= new_session_url %>"> Go to Sign in! </a>

<form class="form" action="<%= users_url %>" method="post">
    <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

    <p>
    <label for='new_user_email'>Email:</label>
    <input type="text" id="ne_user_email" name="user[email]">
    </p>
    <br />

    <p>
    <label for='new_user_password'> Password:</label>
    <input type="password" id="new_user_password" name="user[password]">
    </p>
    <br />


    <input type='Submit'>

</form>


